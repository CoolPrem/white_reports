<table id='section_show' class="one-column-emphasis" summary="Class Details">
    <colgroup>
    	<col class="oce-first" />
    </colgroup>
    <thead>
    	<tr>
        	<th scope="col">Name</th>
            <th scope="col"> <%= clazz.name %></th>
        </tr>
    </thead>
    <tbody>
    	<tr>
        	<td>Branch</td>
            <td>	 <%= link_to clazz.branch.name, branch_path(clazz.branch) %> </td>
        </tr>    	
    	<tr>
        	<td> Institution </td>
            <td>	 <%= link_to clazz.branch.institution.name, institution_path(clazz.branch.institution) %> </td>
        </tr>           
    </tbody>
</table>


