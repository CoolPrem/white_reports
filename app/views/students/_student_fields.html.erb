<tr>
	<td> 
		<strong> <%= f.label :name %> </strong>
	<td> 
		<%= f.text_field :name %> 
	</td>
</tr>
		
<tr>
	<td> 
		<strong> <%= f.label :id_no, "ID number"%> </strong> 
	</td> 
	<td> 
			<%= f.text_field :id_no %>
	</td>
</tr>			

<tr>
	<td> 
		<strong><%= f.label :female, "Gender" %></strong> 
	</td> 
	<td> 	
			<%= f.select :female, options_for_select([  ["Female", true], ["Male", false] ], f.object.female? ) , {:prompt => "Gender"}  %>
	</td>
</tr>			

<tr>
	<td> 
		<strong><%= f.label :dob, "DOB"%></strong> 
	</td>
	<td> 	 
			<%= f.date_select :dob, {:prompt => true, :start_year => Time.now.year - 30, 
											:end_year => Time.now.year, :order => [:day, :month, :year]} %> 
	</td>
</tr>

 
	