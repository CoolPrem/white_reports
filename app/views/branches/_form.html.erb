<%= form_for(@branch) do |f| %>
	<%= render 'shared/error_messages', :target => @branch %>
	  
  	<div class="field">
		<%= f.label :name %><br />
	    <%= f.text_field :name %>
	 </div>
	 
	 <div class="field">
	    <%= f.label :id_no, "ID" %><br />
	    <%= f.text_field :id_no %>
	</div>
	
	<div class="field">
	    <%= f.label :tipe, "Type" %><br />
	    <%= f.text_field :tipe %>
	</div>
	  
	<div class="field">
	    <%= f.label :principal %><br />
	    <%= f.text_field :principal %>
	</div>
	  
	<div class="field">
	    <%= f.label :address %><br />
	    <%= f.text_field :address %>
	</div> 
	
	<div class="field">
	    <%= f.label :city %><br />
	    <%= f.text_field :city %>
	</div> 
	
	<div class="field">
	    <%= f.label :state %><br />
	    <%= f.text_field :state %>
	</div> 
	
	<div class="field">
	    <%= f.label :pin %><br />
	    <%= f.text_field :pin %>
	</div> 
		  
	<div class="actions">
	    <%= f.submit %>
	</div>
<% end %>
